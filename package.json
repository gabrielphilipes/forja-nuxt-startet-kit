{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "prepare": "husky",
    "commit": "cz",
    "lint:prettier": "prettier --check \"**/*.{js,jsx,ts,js,tsx,vue,css,scss,json,md}\"",
    "lint:prettier:fix": "prettier --write \"**/*.{js,jsx,ts,js,tsx,vue,css,scss,json,md}\"",
    "lint:eslint": "eslint .",
    "lint:eslint:fix": "eslint --fix ."
  },
  "dependencies": {
    "nuxt": "^3.17.5",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@nuxt/eslint": "1.4.1",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "9.29.0",
    "husky": "9.1.7",
    "prettier": "3.6.1",
    "secretlint": "10.1.1",
    "@secretlint/secretlint-rule-preset-recommend": "10.1.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
